{
  "name": "a-remarkable-express-server",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start:dev": "ts-node-dev -P ./config/tsconfig.json --respawn --pretty --transpile-only src/index.ts",
    "lint:eslint": "yarn run eslint src -c config/.eslintrc --ignore-path config/.eslintignore",
    "lint:tsc": "tsc --noEmit -p ./config",
    "lint": "yarn run lint:eslint && yarn run lint:tsc"
  },
  "dependencies": {
    "apollo-server-express": "^3.13.0",
    "cookie-parser": "~1.4.4",
    "cors": "^2.8.5",
    "debug": "~4.3.4",
    "express": "^4.19.2",
    "express-graphql": "^0.12.0",
    "graphql": "^16.8.1",
    "helmet": "^7.1.0",
    "http-errors": "~2.0.0",
    "morgan": "~1.10.0",
    "nodemon": "^3.1.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.21",
    "@types/helmet": "^4.0.0",
    "@types/node": "^20.11.30",
    "@typescript-eslint/eslint-plugin": "^7.1.0",
    "@typescript-eslint/parser": "^7.1.0",
    "dotenv": "^16.4.5",
    "eslint": "^8.57.0",
    "eslint-config-standard-with-typescript": "^43.0.1",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-n": "^15.0.0",
    "eslint-plugin-promise": "^6.0.0",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.4.3"
  }
}
